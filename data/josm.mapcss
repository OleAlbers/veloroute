meta {
  title: "Veloroute.Hamburg v3";
}

canvas {
  default-points: false;
  fill-color: #efe9e1;
}

node|z16-[type!=mapillary-sequence] {
  symbol-size:2;
  symbol-shape: square;
  symbol-stroke-color: #333;
  symbol-fill-color: #ccc;
}

node[type=marker] {
  symbol-size: 17;
  symbol-shape: square;
  color: #ff00ee;
  font-size: 10;

  text-halo-radius: 1;

  symbol-stroke-color: #fff;
  symbol-fill-color: eval(tag("color"));
  text: name;
  text-offset-x: -1;
  text-anchor-horizontal: center;
  text-anchor-vertical: center;
}

node|z13-[target]::routing {
  font-size: 15;
  text-halo-radius: 1;
  text: target;
}

node|z16-[target]::routing {
  text-offset-y: 15;
}

way node[type=marker] {
  symbol-shape: circle;
}

/* default way color is purple */
way {
  color: #9233CC;
  width: 3;
  casing-width: 1;
  casing-color: white;
  opacity: 1;
  text-halo-radius: 1;
}

way|z0- {
  font-size: 0;
}

way|z15- {
  font-size: 8;
}

way|z17- {
  font-size: 12;
  font-weight: bold;
}

relation >[role="forward"] way|z15-::oneway {
  modifier: true;
  offset: 0 - prop("width", "default");
  width: prop("width", "default")/2;
  dashes: 15,15;
}

relation >[role="backward"] way|z15-::oneway {
  modifier: true;
  offset: prop("width", "default");
  width: prop("width", "default")/2;
}

/* highlight if mapillary ref is wrong */
way[eval(length(tag("seq_forward")) != 22)],
way[eval(length(tag("img_forward_start")) != 22)],
way[eval(length(tag("img_forward_end")) != 22)],
way[eval(length(tag("seq_backward")) != 22)],
way[eval(length(tag("img_backward_start")) != 22)],
way[eval(length(tag("img_backward_end")) != 22)] {
  set error;
  text: concat(prop(text), " invalid mapillary tag");
}

way[oneway?][seq_forward] {
  color: #7E67A7;
  set ok;
}

way[!oneway?][seq_forward][seq_backward] {
  set ok;
  color: #7E67A7;
}

way[oneway?][seq_backward] {
  set error;
  text: concat(prop(text), " oneway has seq_backward tag");
}

/* img arrow icons forward */
way[seq_forward][!img_forward_start] >[index=1] node|z16-::fw {
  set hasSeqOne;
}
way[seq_forward][!img_forward_end] >[index=-1] node|z16-::fw {
  set hasSeqTwo;
}
way[seq_forward][img_forward_end] >[index=-1] node|z16-::fw {
  set forwardEnd;
}
way[seq_forward][img_forward_start] >[index=1] node|z16-::fw {
  set forwardStart;
}
way[seq_forward][img_forward_end][oneway?] >[index=-1] node|z16-::fw {
  set onewayEnds;
}
way[seq_forward][img_forward_start][oneway?] >[index=1] node|z16-::fw {
  set onewayStarts;
}
node.forwardStart::fw {
  text: eval("↦");
}
node.forwardEnd::fw {
  text: eval("⇥");
}
node.forwardStart.forwardEnd::fw {
  text: eval("⇸");
}
node.hasSeqOne.hasSeqTwo::fw,
node.forwardStart.forwardEnds.onewayEnds::fw,
node.forwardStart.forwardEnds.onewayStarts::fw {
  text: eval("↷");
}
node.forwardStart::fw, node.forwardEnd::fw, node.hasSeqOne.hasSeqTwo::fw {
  text-offset-x: 10;
  text-offset-y: 0;
  text-color: #038D50; /* some green */

  font-size: 20;
  font-weight: bold;
  text-halo-color: #ffffff;
  text-halo-radius: 2;
  text-anchor-horizontal: center;
  text-anchor-vertical: center;
}

/* img arrow icons backward */
way[seq_backward][!img_backward_end] >[index=1] node|z16-::bw {
  set hasSeqOne;
}
way[seq_backward][!img_backward_start] >[index=-1] node|z16-::bw {
  set hasSeqTwo;
}
way[seq_forward][img_forward_end][oneway?] >[index=-1] node|z16-::bw {
  set onewayEnds;
}
way[seq_forward][img_forward_start][oneway?] >[index=1] node|z16-::bw {
  set onewayStarts;
}
way[seq_backward][img_backward_end] >[index=1] node|z16-::bw {
  set backwardEnd;
}
way[seq_backward][img_backward_start] >[index=-1] node|z16-::bw {
  set backwardStart;
}
node.backwardStart::bw {
  text: eval("↤");
}
node.backwardEnd::bw {
  text: eval("⇤");
}
node.backwardStart.backwardEnd::bw,
node.backwardEnd.onewayStarts::bw,
node.backwardStart.onewayEnds::bw   {
  text: eval("⇷");
}
node.hasSeqOne.hasSeqTwo::bw {
  text: eval("↶");
}
node.backwardStart::bw, node.backwardEnd::bw, node.hasSeqOne.hasSeqTwo::bw {
  text-offset-x: -10;
  text-offset-y: 0;
  text-color: #004f8d; /* some blue */
  font-size: 20;
  font-weight: bold;
  text-halo-color: #ffffff;
  text-halo-radius: 2;
  text-anchor-horizontal: center;
  text-anchor-vertical: center;
}

way.ok[color] {
  color: eval(tag("color"));
}

way.ok[!color], way.parentColor.ok[!color]::oneway {
  color: eval(
    count(parent_tags("color")) <= 2 && count(parent_tags("color")) > 0
      ? get(parent_tags("color"), 0)
      : #ff00ee
    );
  dashes: eval(
    count(parent_tags("color")) == 2
      ? list(10, 10)
      : none
    );
  dashes-background-color: get(parent_tags("color"), 1);
}

way.error {
  color: #ff00ee;
}

way[img_date] {
  text: concat(prop(text), eval(tag("img_date")));
}

way[type=mapillary-sequence] {
  color: #00cc63;
  width: 1;
  text: "";
  casing-width: 0;
  opacity: 0.2;
}

way[type=mapillary-sequence] > node|z18- {
  /*
  SVGSalamader doesn't parse this SVG for some reason and doesn't give a decent error message
  icon-image: "data:image/svg+xml,<svg><path d='m -16.745552,-26.259853 a 83.120819,84.457169 0 0 1 -83.120815,-2e-6 l 41.560409,-73.142054 z' transform='rotate(-180 -8 -7)' fill='#00cc63'/></svg>";
  */
  /* same image as png */
  icon-image: "data:image/png;base64,";
  icon-width: 11;
  icon-height: 11;
  icon-opacity: 0.4;
  icon-rotation: degree_to_radians(tag(bearing));
  color: #ff00ee;
  font-size: 10;
}

way[type=mapillary-sequence] > node|z18- {
  icon-width: 15;
  icon-height: 15;
}

way[type=mapillary-sequence] > node|z19- {
  icon-width: 20;
  icon-height: 20;
}

way[gpx:name] {
  color: #FFFE9D;
  text: tag("gpx:name");
  text-position: line;
  width: 1;
  casing-color: #555555;
  casing-width: 0.1;
}

way[gpx:name] > node {
  symbol-size: 1;
  symbol-stroke-opacity: 0;
  symbol-fill-opacity: 0;
  symbol-shape: circle;
}

way[type=detour] {
  color: #f0ca00;
  width: 4;
  text: "";
  casing-width: 0;
  dashes: 2;
}


area[type=article] {
  fill-color: #000;
  fill-opacity: 0.2;
  color: #888;
  width: 1;
  casing-width: 0;
  text: prop(name);
}
